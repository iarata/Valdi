{
  "name": "@snap/eslint-plugin-valdi",
  "version": "1.0.3",
  "description": "ESLint rules for the Valdi project",
  "repository": {
    "type": "git",
    "url": "https://github.com/Snapchat/Valdi.git",
    "directory": "npm_modules/eslint-plugin-valdi"
  },
  "publishConfig": { "access": "public" },
  "main": "dist/index.js",
  "scripts": {
