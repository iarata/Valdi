    "prepare": "npm run build",
    "preinstall": "npm run build"
  },
  "author": "elee2@snapchat.com",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@typescript-eslint/utils": "^6.21.0",
    "eslint": "^8.27.0",
    "typescript": "^5.3.3"
  }
}
